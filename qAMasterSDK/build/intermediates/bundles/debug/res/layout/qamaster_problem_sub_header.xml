<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/qamaster_problem" >

    <TextView
        android:id="@+id/qamaster_report_bug_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/qamaster_dialog_panel_bg"
        android:paddingBottom="4dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="12dp"
        android:text="@string/qamaster_problem_includes"
        android:textColor="@color/qamaster_dialog_panel_text"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/qamaster_report_bug_test_cycle_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/qamaster_dialog_panel_bg"
        android:paddingBottom="12dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:textColor="@color/qamaster_dialog_panel_text"
        android:textSize="12sp"
        android:visibility="gone"
        tools:text="TestCycle"
        tools:visibility="visible" />

</merge>